{
 "op": "seq",
 "seq": [
  {
   "op": "set",
   "var": "iconStyle",
   "value": {
    "choices": {
     "op": "array",
     "values": [
      "control",
      "hoverBox",
      "magnifier"
     ]
    },
    "weights": {
     "op": "array",
     "values": [
      0.3,
      0.35,
      0.35
     ]
    },
    "unit": {
     "op": "get",
     "var": "userId"
    },
    "op": "weightedChoice"
   }
  },
  {
   "op": "cond",
   "cond": [
    {
     "if": {
      "op": "equals",
      "left": {
       "op": "get",
       "var": "iconStyle"
      },
      "right": "control"
     },
     "then": {
      "op": "seq",
      "seq": [
       {
        "op": "set",
        "var": "galleryLayout",
        "value": "control"
       }
      ]
     }
    },
    {
     "if": true,
     "then": {
      "op": "seq",
      "seq": [
       {
        "op": "set",
        "var": "galleryLayout",
        "value": {
         "choices": {
          "op": "array",
          "values": [
           "alts",
           "alts-colors",
           "alts-colors-avail"
          ]
         },
         "unit": {
          "op": "get",
          "var": "userId"
         },
         "op": "uniformChoice"
        }
       }
      ]
     }
    }
   ]
  }
 ]
}
